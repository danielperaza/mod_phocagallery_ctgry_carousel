{"ts":1352671344827,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<?xml version=\"1.0\" encoding=\"utf-8\"?>\r<extension type=\"module\" client=\"site\" version=\"2.5.0\" method=\"upgrade\">\r\t<name>Phoca Gallery Categories Display</name>\r\t<author>Jan Linhart aka Escope.cz</author>\r\t<creationDate>Mar.2011</creationDate>\r\t<copyright>Copyright (C) Jan Linhart.</copyright>\r\t<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>\r\t<authorEmail>admin@escope.cz</authorEmail>\r\t<authorUrl>www.escope.cz</authorUrl>\r\t<version>2.5.2</version>\r\t<description>Shows links, thumbnails, description from PhocaGallery categories.</description>\r\t<files>\r\t\t<filename module=\"mod_phocagallery_ctgry\">mod_phocagallery_ctgry.php</filename> \r\t\t<filename>index.html</filename>\r\t\t<filename>helper.php</filename>\r\t\t<filename>changelog.txt</filename>\r\t\t<folder>css</folder>\r\t\t<folder>images</folder>\r\t\t<folder>js</folder>\r\t\t<folder>tmpl</folder>\r\t</files>\r        <config>\r\t<fields name=\"params\">\r            <fieldset name=\"basic\">\r                <field name=\"moduleclass_sfx\" type=\"text\" default=\"\" label=\"Module Class Suffix\" description=\"fieldMODULECLASSSUFFIX\" />\r                <field name=\"insert_jquery\" type=\"radio\" default=\"1\" label=\"Insert Jquery library\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"round_corners\" type=\"radio\" default=\"1\" label=\"Round corners\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"show_catname\" type=\"radio\" default=\"1\" label=\"Show category name\" description=\"Display category name or not\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"show_read_more\" type=\"radio\" default=\"1\" label=\"Show read more\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"show_description\" type=\"radio\" default=\"1\" label=\"Show description\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"show_date\" type=\"radio\" default=\"1\" label=\"Show date\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"date_format\" type=\"text\" default=\"%a %d %b %Y - %H:%M\" label=\"Date format\" description=\"Formate date like this %a %d %b %Y - %H:%M\"></field>\r                <field name=\"show_image_count\" type=\"radio\" default=\"1\" label=\"Show image count\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"show_comment_count\" type=\"radio\" default=\"1\" label=\"Show comment count\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"comments_desc\" type=\"text\" default=\"[number] comments\" label=\"Comments description\" description=\"Shows if comment count is showed. Tag [number] will be replaced by actual number.\"></field>\r                <field name=\"show_hit_count\" type=\"radio\" default=\"1\" label=\"Show hit count\" description=\"\">\r                  <option value=\"0\">No</option>\r                  <option value=\"1\">Yes</option>\r                </field>\r                <field name=\"hits_desc\" type=\"text\" default=\"[number] hits\" label=\"Hits description\" description=\"Shows if hit count is showed. Tag [number] will be replaced by actual number.\"></field>\r                <field name=\"order\" type=\"list\" default=\"ASC\" label=\"Order of appearence\" description=\"\">\r                  <option value=\"ASC\">Ascending</option>\r                  <option value=\"DESC\">Descending</option>\r                  <option value=\"RAND()\">Random</option>\r                </field>\r                <field name=\"style\" type=\"filelist\" directory=\"/modules/mod_phocagallery_ctgry/css/\" default=\"style1.css\" label=\"Select css style\" description=\"\">\r                </field>\r                <field name=\"thumbnail\" type=\"list\" default=\"m\" label=\"Thumbnails\" description=\"\">\r                  <option value=\"s\">Small</option>\r                  <option value=\"m\">Medium</option>\r                  <option value=\"l\">Large</option>\r                </field>\r                <field name=\"category_display\" type=\"list\" default=\"all\" label=\"Display categories\" description=\"\">\r                  <option value=\"all\">All</option>\r                  <option value=\"selected\">Selected onely</option>\r                  <option value=\"not_selected\">Not selected onely</option>\r                  <option value=\"subcategories\">Subcategories of selected ones</option>\r                </field>\r                <field\r                    name=\"categories\"\r                    type=\"sql\"\r                    query=\"SELECT id, title FROM #__phocagallery_categories\"\r                    key_field=\"id\" value_field=\"title\"\r                    multiple=\"multiple\"\r                    label=\"Categories\"\r                    description=\"Use Ctrl-click to select more than one item.\">\r                </field>\r                <field\r                    name=\"Itemid\"\r                    type=\"sql\"\r                    query=\"SELECT id, title FROM #__menu WHERE client_id = 0 AND link LIKE '%com_phocagallery%'\"\r                    key_field=\"id\" value_field=\"title\"\r                    label=\"Menu item setting\"\r                    description=\"Phoca Gallery takes setting like pagination from menu item. You can define how should galleries behave in menu item selected here. It can be menu item in menu you don't use on frontend.\">\r                </field>\r                <field name=\"description_length\" type=\"text\" default=\"140\" label=\"Description length\" description=\"Must be number. Number of chars.\"></field>\r                <field name=\"limit\" type=\"text\" default=\"3\" label=\"Number of categories\" description=\"Must be number.\"></field>\r                <field name=\"limit_images\" type=\"text\" default=\"1\" label=\"Number of images\" description=\"Must be number.\"></field>\r                <field name=\"description_end\" type=\"text\" default=\"...\" label=\"End of description\" description=\"Can be any string.\"></field>\r                <field name=\"read_more\" type=\"text\" default=\"read more\" label=\"Read more text\" description=\"Can be any string.\"></field>\r                <field name=\"border_width\" type=\"text\" default=\"3\" label=\"Border width\" description=\"Work onely if 'Round corners' is checked. Must be number. In pixels.\"></field>\r                <field name=\"corner_radius\" type=\"text\" default=\"4\" label=\"Corner radius\" description=\"Work onely if 'Round corners' is checked. Must be number. In pixels.\"></field>\r                <field name=\"border_color\" type=\"text\" default=\"#d5d5d5\" label=\"Border color\" description=\"Work onely if 'Round corners' is checked. Hexadecimal color like #b9009d.\"></field>\r            </fieldset>\r        </fields>\r    </config>\r    <updateservers> \r    <server type=\"extension\" priority=\"1\" name=\"Phoca Gallery Categories Display\">http://updateserver.escope.cz/mod_phocagallery_ctgry/master.xml</server>\r